{
  "name": "firecrud",
  "version": "1.0.0",
  "description": "FIREcrud",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"cd server && npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix server && npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylanprem/FIREcrud.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dylanprem/FIREcrud/issues"
  },
  "homepage": "https://github.com/dylanprem/FIREcrud#readme",
  "dependencies": {
    "concurrently": "^4.1.0"
  }
}
